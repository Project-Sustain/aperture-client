        "group":"Dynamic Layers",
        "subGroup": "Climate",
        "constraints": {
            "temperature": {
                "type": "slider",
                "range": [
                    88,
                    110
                ],
                "default": [
                    88
                ],
                "step": 1
            },
            "length": {
                "type": "slider",
                "label": "Length (Days)",
                "range": [
                    1,
                    200
                ],
                "default": [
                    1
                ],
                "step": 1
            },
            "years": {
                "type": "slider",
                "label": "Yearly Range",
                "range": [
                    2006,
                    2010
                ],
                "default": [
                    2006,
                    2010
                ],
                "step": 1
            }
        },
        "onConstraintChange": "censusViz.updateFutureHeat(RenderInfrastructure.map, true)"
